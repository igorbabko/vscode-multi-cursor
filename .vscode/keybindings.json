[
  {
    "key": "ctrl+r",
    "command": "workbench.action.reloadWindow"
  },
  {
    "args": "<C-d>",
    "command": "vscode-neovim.send",
    "key": "cmd+d",
    "when": "editorFocus && neovim.init"
  },
  {
    "args": "<CS-d>",
    "command": "vscode-neovim.send",
    "key": "cmd+shift+d",
    "when": "editorFocus && neovim.init"
  },
  {
    "args": "<CS-l>",
    "command": "vscode-neovim.send",
    "key": "cmd+shift+l",
    "when": "editorFocus && neovim.init"
  }
]
